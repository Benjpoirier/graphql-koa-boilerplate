{
    "name": "graphql-koa-boilerplate",
    "version": "0.0.1",
    "description": "graphQL api with koajs & typeorm v0.1beta",
    "main": "index.js",
    "scripts": {
        "install:docker": "@echo \"docker compose\" && docker-compose up -d --no-recreate",
        "lint:server": "tslint -c ./tslint.json './src/**/*.ts'",
        "start:dev": "NODE_ENV=developpment nodemon",
        "start:prod": "NODE_ENV=production ts-node -r tsconfig-paths/register ./src/main.ts",
        "test": "echo \"Error: no test specified\" && exit 1",
        "database:regenerate": "ts-node ./src/initializers/regenerateDb.ts",
        "migration:run": "ts-node ./node_modules/.bin/typeorm migration:run",
        "migration:revert": "ts-node ./node_modules/.bin/typeorm migration:revert"
    },
    "author": "benjamin poirier",
    "license": "ISC",
    "devDependencies": {
        "@types/ioredis": "^3.2.9",
        "@types/kcors": "^2.2.3",
        "@types/koa": "^2.0.46",
        "@types/koa-bodyparser": "^5.0.0",
        "@types/koa-compress": "^2.0.8",
        "@types/koa-etag": "^3.0.0",
        "@types/koa-helmet": "^3.1.2",
        "@types/koa-router": "^7.0.30",
        "@types/node": "^10.3.4",
        "nodemon": "^1.17.5",
        "tslint": "^5.10.0",
        "typescript": "2.5.2"
    },
    "dependencies": {
        "apollo-server-koa": "^1.3.6",
        "ts-node": "^6.1.2",
        "graphql": "^0.13.2",
        "graphql-tools": "^3.0.2",
        "ioredis": "^3.2.2",
        "kcors": "^2.2.1",
        "koa": "^2.5.1",
        "koa-bodyparser": "^4.2.1",
        "koa-compress": "^3.0.0",
        "koa-etag": "^3.0.0",
        "koa-helmet": "^4.0.0",
        "koa-ratelimit": "^4.1.2",
        "koa-router": "^7.4.0",
        "pg": "^7.4.3",
        "reflect-metadata": "^0.1.10",
        "tsconfig-paths": "^3.4.0",
        "typeorm": "0.2.7",
        "typescript": "^2.9.2"
    }
}
